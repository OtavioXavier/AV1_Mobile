<resources>
    <declare-styleable name="UserPosition">
        <!-- Latitude do usuário -->
        <attr name="Latitude" format="string" />

        <!-- Longitude do usuário -->
        <attr name="Longitude" format="string" />

        <!-- Velocidade do usuário -->
        <attr name="Velocidade" format="string" />

        <!-- Exemplo de string -->
        <attr name="exampleString" format="string" />

        <!-- Exemplo de cor -->
        <attr name="exampleColor" format="color" />

        <!-- Exemplo de dimensão -->
        <attr name="exampleDimension" format="dimension" />

        <!-- Exemplo de drawable -->
        <attr name="exampleDrawable" format="reference" />
    </declare-styleable>
</resources>
